<?xml version="1.0" encoding="UTF-8"?>
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
  
<bean id="BookBean" class="com.demo.Book">  
<property name="bookid" value="1001"></property>  
<property name="bookname" value="java"></property>  
<property name="author" value="james"></property>  


</bean>  
<bean id="BookBean1" class="com.demo.Book">  
<property name="bookid" value="1002"></property>  
<property name="bookname" value="c++"></property>  
<property name="author" value="john"></property>  


</bean>  
<bean id="book1" class="com.demo.Book">
        <constructor-arg value="98765"/>
      	<constructor-arg value="Programming in Java"/>
      	<constructor-arg value="A.B.DAS"/>
      </bean>  
  
</beans>  

